{
    "package": {
        "name": "packages",
        "repo": "debian", 
        "subject": "rackhd"
    },

    "version": {
        "name": "#REVERSION#"
    },

    "files":
    [
        {
            "includePattern": "deb/(.*)", "uploadPattern": "$1", 
            "matrixParams": 
            { 
                "override": 1, 
                "deb_distribution": "trusty",
                "deb_component": "#COMPONENT#",
                "deb_architecture": "amd64"
            }
        }
    ],
    "publish": true
}
